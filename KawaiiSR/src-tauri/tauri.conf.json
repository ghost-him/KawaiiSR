{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "kawaiisr",
  "version": "0.1.0",
  "identifier": "com.ghost-him.kawaiisr",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "KawaiiSR",
        "width": 1200,
        "height": 800
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "msi",
    "resources": [
      "onnx/KawaiiSR-precise.onnx",
      "models.toml"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}